// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "node_modules": true,
        "**/bin": true,
        "**/coverage": true,
        "**/obj": true,
        "**/compilerconfig.*": true,
        "**/packages.config": true,
        "**/*.csproj": true,
        "**/*.user": true,
        "**/*.css": {
            "when": "$(basename).less"
        },
        "**/*.min.css": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*?.js": {
            "when": "$(basename).tsx"
        },
        "**/*.js.map": true
    }
}